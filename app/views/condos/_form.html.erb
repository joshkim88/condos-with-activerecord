<%= simple_form_for @condo do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :price %>
  <%= f.number_field :price %>
  <%= f.label :bedroom %>
  <%= f.number_field :bedroom %>
  <%= f.label :bathroom %>
  <%= f.number_field :bathroom %>
  <%= f.label :size %>
  <%= f.number_field :size %>
  <%= f.submit %>
<% end %>
